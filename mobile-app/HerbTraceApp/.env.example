# HerbTrace Mobile App Environment Variables Example
# Copy this file to .env and update with your actual values

# Development Backend URL (your local IP - update this to your computer's IP)
EXPO_PUBLIC_API_DEV_URL=http://192.168.1.100:8001

# Production Backend URL (Railway deployment - update after deployment)
EXPO_PUBLIC_API_PROD_URL=https://your-app.railway.app

# API Configuration
EXPO_PUBLIC_API_TIMEOUT=10000
EXPO_PUBLIC_CONNECTION_TIMEOUT=5000

# App Configuration
EXPO_PUBLIC_APP_NAME=HerbTrace
EXPO_PUBLIC_APP_VERSION=1.0.0

# Debug Settings
EXPO_PUBLIC_DEBUG_LOGS=true